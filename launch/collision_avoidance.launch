    <param name="STOP_AND_WAIT" value="$(arg STOP_AND_WAIT)"/>
    <param name="SAW_PRIO" value="7"/>   
  </node>

  <!-- RViz -->
  <node if="$(arg enableRviz)" pkg="rviz" type="rviz" name="rviz" args="-d $(find multi_robot_system)/scene/MRS.rviz"/>

</launch>
